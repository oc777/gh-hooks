<h1>1dv023/oc222ba-examination-3</h1>
<div class="issues">
    {{#if locals.issues.length }}
        {{#each locals.issues}}
            <div class="issue">
                <div>
                    <span><strong>url:</strong> {{this.url}}</span>
                </div>
                <div>
                    <span><strong>title:</strong> {{this.title}}</span>
                </div>
                <div>
                    <span><strong>state:</strong> {{this.state}}</span>
                </div>
            </div>
        {{/each}}
    {{else}}
        <p>no issues</p>
    {{/if}}
</div>
<div class="payload">

</div>
